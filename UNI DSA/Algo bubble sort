#include<iostream>
using namespace std;
int main(){
    int n=7;
    int arr[n]={7,9,4,6,2,10,7};
    bool swapped=false;
    
    for (int i=0; i<n; i++){
        for(int j=0; j<n-i; j++){
            if(arr[j]>arr[j+1]){
                swap(arr[j],arr[j+1]);
                swapped=true;
            }
        }
    }
    if(swapped=false){
        cout<<"the array is alraedy swapped"<<endl;
    }
    
    for(int i=1; i<=n; i++){
        cout<<arr[i]<<endl;
    }
    
}  
